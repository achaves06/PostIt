<%= form_for @post do |f| %>
  <%= render 'shared/error_messages', object: f.object %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :url %>
  <%= f.text_field :url %>
  <%= f.label :description %>
  <%= f.text_area :description, rows: 5 %>
<br/>
  <%= f.submit "Create Post", class: "btn btn-primary" %>
<% end %>
